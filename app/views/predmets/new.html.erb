<h3 style="margin-left:10px;">Nový předmět</h3>

<%= render "form", predmet: @predmet %>




<div class="card" style="width:370px;margin-left:10px;margin-top:5px;padding:10px;">
  <h5>Importovat předměty</h5>
  <p>
    <span >Můžete importovat předměty z csv souborů</span>
    <%= form_tag import_predmets_path, multipart: true do %>
      <%= file_field_tag :file %>
      <%= submit_tag "Import CSV" ,class:"btn btn-success" ,style:"margin-top:5px;"%>
    <% end %>
  </p>
</div >


